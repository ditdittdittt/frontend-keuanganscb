    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.12.2",
    "vue-chartjs": "^3.5.0",
    "vue-signature-pad": "^2.0.2"
  },
  "devDependencies": {
    "@mdi/font": "^5.3.45",
    "@nuxtjs/eslint-config": "^2.0.0",
